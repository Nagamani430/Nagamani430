import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner str=new Scanner(System.in);
        String s=str.nextLine();
        String vowel="aeiou";
        for(int i=0;i<s.length();i++)
        {
        char ch=s.charAt(i);
            if(vowel.indexOf(ch)==-1)
            System.out.print(ch);
        }
    }
}
