class StringExample
{
public static void main(String[] args)
{
 String s="aaabbccb";
 int n=s.length();
for(int i=0;i<n;i++){
System.out.println(s.indexOf('b'));
System.out.println(s.lastIndexOf('b'));
 int idx=s.indexOf('b');
System.out.println(s.indexOf('b',idx+1));
System.out.println(s.charAt(idx+1));
}
}
}
