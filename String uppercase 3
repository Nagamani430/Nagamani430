import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner str=new Scanner(System.in);
        String s1=str.nextLine();
          for(int i=0;i<s1.length();i++)
        {
              s1=s1.toUpperCase();
        char ch=s1.charAt(i);    
            System.out.print(ch);
        }
        
        
    }
}
